@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_RootLayout.cshtml";
}
<div class="signup">
    <h3>
        企业会员注册<span><a href="#">网站资费</a></span></h3>
    <form>
    <div class="need_write">
        <img src="images/need_img.jpg" /><span>=必须项</span></div>
    <table cellpadding="0" cellspacing="0" class="register_table">
        <tr>
            <td align="right">
                电子邮件：
            </td>
            <td>
                <input type="text" class="need_input" />
            </td>
        </tr>
        <tr>
            <td align="right">
                密码：
            </td>
            <td>
                <input type="password" class="need_input" />
            </td>
        </tr>
        <tr>
            <td align="right">
                重复密码：
            </td>
            <td>
                <input type="password" class="need_input" />
            </td>
        </tr>
        <tr>
            <td align="right">
                单位名称：
            </td>
            <td>
                <input type="text" class="need_input" />
            </td>
        </tr>
        <tr>
            <td align="right">
                您的职位：
            </td>
            <td>
                <select>
                    <option></option>
                </select>
            </td>
        </tr>
        <tr>
            <td align="right">
                联系人：
            </td>
            <td>
                <input type="text" class="need_input" />
            </td>
        </tr>
        <tr>
            <td align="right">
                联系电话：
            </td>
            <td>
                <input type="text" class="need_input_short" /><span>分机：</span><input type="text"
                    class="text_tel" />
            </td>
        </tr>
        <tr>
            <td align="right">
                详细地址：
            </td>
            <td>
                <input type="text" class="need_input" />
            </td>
        </tr>
        <tr>
            <td align="right">
                邮编：
            </td>
            <td>
                <input type="text" class="text_post" />
            </td>
        </tr>
    </table>
    <div class="line">
    </div>
    <div class="register_btns">
        <input type="image" src="images/register_btns.jpg" /></div>
    </form>
</div>
